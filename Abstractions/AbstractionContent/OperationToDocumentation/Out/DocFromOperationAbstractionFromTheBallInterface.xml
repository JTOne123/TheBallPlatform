<?xml version="1.0"?>
<DocumentationAbstraction xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://tempuri.org/Operation_v1_0.xsd">
  <Documentations>
    <Documents>
      <Document name="Operations (OperationAbstractionFromTheBallInterface)" title="Operations (OperationAbstractionFromTheBallInterface)">
        <Content>
          <Header text="ExecuteOperation (OwnerLocation, OperationDomain, OperationName, OperationParameters, CallerProvidedInfo)" level="1">
            <Header text="Specifications" level="2">
              <Paragraph>
                <Text>
                  <TextContent>Execute given operation</TextContent>
                </Text>
              </Paragraph>
            </Header>
            <Header text="Parameters" level="2">
              <Paragraph>
                <Table>
                  <Columns>
                    <Column name="Parameter" />
                    <Column name="DataType" />
                    <Column name="Description" />
                  </Columns>
                  <Rows>
                    <Row>
                      <ColumnData styleRef="color:blue;font-weight:bold;font-style:italic">
                        <TextContent>OwnerLocation</TextContent>
                      </ColumnData>
                      <ColumnData styleRef="color:blue;font-weight:bold;font-style:italic">
                        <TextContent>string</TextContent>
                      </ColumnData>
                      <ColumnData styleRef="color:blue;font-weight:bold;font-style:italic">
                        <TextContent>Owner location of the operation context</TextContent>
                      </ColumnData>
                    </Row>
                    <Row>
                      <ColumnData styleRef="color:blue;font-weight:bold;font-style:italic">
                        <TextContent>OperationDomain</TextContent>
                      </ColumnData>
                      <ColumnData styleRef="color:blue;font-weight:bold;font-style:italic">
                        <TextContent>string</TextContent>
                      </ColumnData>
                      <ColumnData styleRef="color:blue;font-weight:bold;font-style:italic">
                        <TextContent>Operation namespace</TextContent>
                      </ColumnData>
                    </Row>
                    <Row>
                      <ColumnData styleRef="color:blue;font-weight:bold;font-style:italic">
                        <TextContent>OperationName</TextContent>
                      </ColumnData>
                      <ColumnData styleRef="color:blue;font-weight:bold;font-style:italic">
                        <TextContent>string</TextContent>
                      </ColumnData>
                      <ColumnData styleRef="color:blue;font-weight:bold;font-style:italic">
                        <TextContent>Operation name</TextContent>
                      </ColumnData>
                    </Row>
                    <Row>
                      <ColumnData styleRef="color:blue;font-weight:bold;font-style:italic">
                        <TextContent>OperationParameters</TextContent>
                      </ColumnData>
                      <ColumnData styleRef="color:blue;font-weight:bold;font-style:italic">
                        <TextContent>byte[]</TextContent>
                      </ColumnData>
                      <ColumnData styleRef="color:blue;font-weight:bold;font-style:italic">
                        <TextContent>Operation parameters binary serialized.</TextContent>
                      </ColumnData>
                    </Row>
                    <Row>
                      <ColumnData styleRef="color:red;font-weight:bold;text-decoration:underline">
                        <TextContent>CallerProvidedInfo</TextContent>
                      </ColumnData>
                      <ColumnData styleRef="color:red;font-weight:bold;text-decoration:underline">
                        <TextContent>string</TextContent>
                      </ColumnData>
                      <ColumnData styleRef="color:red;font-weight:bold;text-decoration:underline">
                        <TextContent>Caller provided information about the call. For caller specific purposes.</TextContent>
                      </ColumnData>
                    </Row>
                  </Rows>
                </Table>
              </Paragraph>
            </Header>
          </Header>
          <Header text="UpdateStatusSummary (Owner, StartTime, EndTime, ChangedIDList, RemoveExpiredEntriesSeconds)" level="1">
            <Header text="Specifications" level="2">
              <Paragraph>
                <Text>
                  <TextContent>Updates status summary for given owner, based on changed ID lists and time range.</TextContent>
                </Text>
              </Paragraph>
            </Header>
            <Header text="Parameters" level="2">
              <Paragraph>
                <Table>
                  <Columns>
                    <Column name="Parameter" />
                    <Column name="DataType" />
                    <Column name="Description" />
                  </Columns>
                  <Rows>
                    <Row>
                      <ColumnData>
                        <TextContent>Owner</TextContent>
                      </ColumnData>
                      <ColumnData>
                        <TextContent>TheBall.CORE.IContainerOwner</TextContent>
                      </ColumnData>
                      <ColumnData>
                        <TextContent>Owner of the operation context</TextContent>
                      </ColumnData>
                    </Row>
                    <Row>
                      <ColumnData>
                        <TextContent>StartTime</TextContent>
                      </ColumnData>
                      <ColumnData>
                        <TextContent>DateTime</TextContent>
                      </ColumnData>
                      <ColumnData>
                        <TextContent>Starting time of changes updated in this cycle</TextContent>
                      </ColumnData>
                    </Row>
                    <Row>
                      <ColumnData>
                        <TextContent>EndTime</TextContent>
                      </ColumnData>
                      <ColumnData>
                        <TextContent>DateTime</TextContent>
                      </ColumnData>
                      <ColumnData>
                        <TextContent>Ending time of changes updated in this cycle</TextContent>
                      </ColumnData>
                    </Row>
                    <Row>
                      <ColumnData>
                        <TextContent>ChangedIDList</TextContent>
                      </ColumnData>
                      <ColumnData>
                        <TextContent>string[]</TextContent>
                      </ColumnData>
                      <ColumnData>
                        <TextContent>Changed ID list on this cycle</TextContent>
                      </ColumnData>
                    </Row>
                    <Row>
                      <ColumnData>
                        <TextContent>RemoveExpiredEntriesSeconds</TextContent>
                      </ColumnData>
                      <ColumnData>
                        <TextContent>int</TextContent>
                      </ColumnData>
                      <ColumnData>
                        <TextContent>Remove entries older than amount of seconds given here</TextContent>
                      </ColumnData>
                    </Row>
                  </Rows>
                </Table>
              </Paragraph>
            </Header>
            <Header text="Target Definition: ChangeItem ( Owner, StartTime, EndTime, ChangedIDList )" level="2">
              <Paragraph>
                <Text>
                  <TextContent>Change item registering the ID changed list. NOTE! ID is of form UTCStart_UTCEnd_GUID</TextContent>
                </Text>
              </Paragraph>
            </Header>
            <Header text="Method Call: StoreObject ( ChangeItem )" level="2">
              <Paragraph>
                <Text>
                  <TextContent>Store changeitem</TextContent>
                </Text>
              </Paragraph>
            </Header>
            <Header text="Method Call: EnsureUpdateOnStatusSummary ( ChangeItem, Owner, RemoveExpiredEntriesSeconds )" level="2">
              <Paragraph>
                <Text>
                  <TextContent>Guarantee update on StatusSummary, retry if fails. Sort the RecentChangeItemIDs to ID order, remove expired entries</TextContent>
                </Text>
              </Paragraph>
              <Paragraph>
                <Table>
                  <Columns>
                    <Column name="Output value field" />
                    <Column name="DataType" />
                    <Column name="Description" />
                  </Columns>
                  <Rows>
                    <Row>
                      <ColumnData>
                        <TextContent>ExpiredEntryIDs</TextContent>
                      </ColumnData>
                      <ColumnData>
                        <TextContent>string[]</TextContent>
                      </ColumnData>
                      <ColumnData>
                        <TextContent>Expired entries based on expiration time</TextContent>
                      </ColumnData>
                    </Row>
                  </Rows>
                </Table>
              </Paragraph>
            </Header>
            <Header text="Method Call: RemoveExpiredEntries ( EnsureUpdateOnStatusSummaryOutput, Owner )" level="2">
              <Paragraph>
                <Text>
                  <TextContent>Remove expired changeitem entries</TextContent>
                </Text>
              </Paragraph>
            </Header>
          </Header>
        </Content>
      </Document>
    </Documents>
  </Documentations>
</DocumentationAbstraction>