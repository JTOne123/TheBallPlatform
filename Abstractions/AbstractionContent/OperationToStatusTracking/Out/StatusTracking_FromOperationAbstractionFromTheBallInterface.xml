<?xml version="1.0"?>
<StatusTrackingAbstraction xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://tempuri.org/StatusTracking_v1_0.xsd">
  <StatusItems scopeName="TheBall.Interface">
    <StatusItem name="ExecuteOperation_OwnerLocation" displayName="VariableType: OwnerLocation" description="Owner location of the operation context">
      <StatusValue trafficLightIndicator="yellow" indicatorDisplayText="Being implemented" indicatorValue="1" />
    </StatusItem>
    <StatusItem name="ExecuteOperation_OperationDomain" displayName="VariableType: OperationDomain" description="Operation namespace">
      <StatusValue trafficLightIndicator="yellow" indicatorDisplayText="Being implemented" indicatorValue="1" />
    </StatusItem>
    <StatusItem name="ExecuteOperation_OperationName" displayName="VariableType: OperationName" description="Operation name">
      <StatusValue trafficLightIndicator="yellow" indicatorDisplayText="Being implemented" indicatorValue="1" />
    </StatusItem>
    <StatusItem name="ExecuteOperation_OperationParameters" displayName="VariableType: OperationParameters" description="Operation parameters binary serialized.">
      <StatusValue trafficLightIndicator="yellow" indicatorDisplayText="Being implemented" indicatorValue="1" />
    </StatusItem>
    <StatusItem name="ExecuteOperation_CallerProvidedInfo" displayName="VariableType: CallerProvidedInfo" description="Caller provided information about the call. For caller specific purposes.">
      <StatusValue trafficLightIndicator="red" indicatorDisplayText="Under design" indicatorValue="1" />
    </StatusItem>
    <StatusItem name="UpdateStatusSummary_Owner" displayName="VariableType: Owner" description="Owner of the operation context">
      <StatusValue trafficLightIndicator="green" indicatorDisplayText="Implemented" indicatorValue="1" />
    </StatusItem>
    <StatusItem name="UpdateStatusSummary_UpdateTime" displayName="VariableType: UpdateTime" description="Starting time of changes updated in this cycle">
      <StatusValue trafficLightIndicator="green" indicatorDisplayText="Implemented" indicatorValue="1" />
    </StatusItem>
    <StatusItem name="UpdateStatusSummary_ChangedIDList" displayName="VariableType: ChangedIDList" description="Changed ID list on this cycle">
      <StatusValue trafficLightIndicator="green" indicatorDisplayText="Implemented" indicatorValue="1" />
    </StatusItem>
    <StatusItem name="UpdateStatusSummary_RemoveExpiredEntriesSeconds" displayName="VariableType: RemoveExpiredEntriesSeconds" description="Remove entries older than amount of seconds given here">
      <StatusValue trafficLightIndicator="green" indicatorDisplayText="Implemented" indicatorValue="1" />
    </StatusItem>
    <StatusItem name="UpdateStatusSummary_EnsureUpdateOnStatusSummary" displayName="MethodExecuteType: EnsureUpdateOnStatusSummary" description="Guarantee update on StatusSummary, retry if fails. Time-stamp blockify the IDs, placing ID aways highest in the graph. Remove expired blocks entirely.">
      <StatusValue trafficLightIndicator="green" indicatorDisplayText="Implemented" indicatorValue="1" />
    </StatusItem>
  </StatusItems>
  <Groups>
    <Group name="ExecuteOperation">
      <ItemRef itemName="ExecuteOperation_OwnerLocation" />
      <ItemRef itemName="ExecuteOperation_OperationDomain" />
      <ItemRef itemName="ExecuteOperation_OperationName" />
      <ItemRef itemName="ExecuteOperation_OperationParameters" />
      <ItemRef itemName="ExecuteOperation_CallerProvidedInfo" />
    </Group>
    <Group name="UpdateStatusSummary">
      <ItemRef itemName="UpdateStatusSummary_Owner" />
      <ItemRef itemName="UpdateStatusSummary_UpdateTime" />
      <ItemRef itemName="UpdateStatusSummary_ChangedIDList" />
      <ItemRef itemName="UpdateStatusSummary_RemoveExpiredEntriesSeconds" />
      <ItemRef itemName="UpdateStatusSummary_EnsureUpdateOnStatusSummary" />
    </Group>
  </Groups>
</StatusTrackingAbstraction>