    </StatusItem>
    <StatusItem name="UpdateStatusSummary_RemoveExpiredEntries" displayName="MethodExecuteType: RemoveExpiredEntries" description="Remove expired changeitem entries">
      <StatusValue trafficLightIndicator="green" indicatorDisplayText="Implemented" indicatorValue="1" />
    </StatusItem>
