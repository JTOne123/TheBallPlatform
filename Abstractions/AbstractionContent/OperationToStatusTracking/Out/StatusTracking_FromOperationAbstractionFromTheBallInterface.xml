      <StatusValue trafficLightIndicator="green" indicatorDisplayText="Implemented" indicatorValue="1" />
    </StatusItem>
    <StatusItem name="UpdateStatusSummary_ChangeItem" displayName="TargetDefinitionType: ChangeItem" description="Change item registering the ID changed list. NOTE! ID is of form UTCStart_UTCEnd_GUID">
