      <ItemRef itemName="UpdateStatusSummary_Owner" />
      <ItemRef itemName="UpdateStatusSummary_StartTime" />
      <ItemRef itemName="UpdateStatusSummary_EndTime" />
      <ItemRef itemName="UpdateStatusSummary_ChangedIDList" />
