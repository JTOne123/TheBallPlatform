<?xml version="1.0"?>
<StatusTrackingAbstraction xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://tempuri.org/StatusTracking_v1_0.xsd">
  <StatusItems scopeName="TheBall.Interface">
    <StatusItem name="ExecuteOperation_OwnerLocation" displayName="VariableType: OwnerLocation" description="Owner location of the operation context">
      <StatusValue trafficLightIndicator="yellow" indicatorDisplayText="Being implemented" indicatorValue="1" />
    </StatusItem>
    <StatusItem name="ExecuteOperation_OperationDomain" displayName="VariableType: OperationDomain" description="Operation namespace">
      <StatusValue trafficLightIndicator="yellow" indicatorDisplayText="Being implemented" indicatorValue="1" />
