<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="CaloomAzure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-05.1.7">
  <WorkerRole name="CaloomWorkerRole" vmsize="ExtraSmall">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="StorageConnectionString" />
    </ConfigurationSettings>
    <LocalResources>
      <LocalStorage name="LocalST" cleanOnRoleRecycle="true" />
    </LocalResources>
  </WorkerRole>
</ServiceDefinition>